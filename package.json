{
  "name": "ai-forgot-this-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:dev": "tsc -b --pretty --sourceMap && vite build --mode development --minify false",
    "build:prod": "tsc -b && vite build --mode production",
